<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:ads="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Page_Profile">



    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar3V"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#FFFFFF"
        android:minHeight="?attr/actionBarSize"
        android:scrollbarStyle="outsideInset"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:title="          Vos information"
        app:titleMargin="5dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="29dp"
        ads:srcCompat="@drawable/line_fine"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar3V"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/toolbar3V"
        app:layout_constraintTop_toTopOf="@+id/toolbar3V"
        app:layout_constraintVertical_bias="1.0" />


    <ImageView
        android:id="@+id/Retourne4"
        android:layout_width="28dp"
        android:layout_height="26dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.045"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.021"
        app:srcCompat="@drawable/ic_arrow_back_black_24dp"/>


    <ScrollView
        android:id="@+id/Scrol_information"
        android:layout_width="match_parent"
        android:layout_height="725dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar3V"
        tools:layout_editor_absoluteX="4dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">


            <TextView
                android:id="@+id/textView13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="20dp"
                android:layout_marginTop="2dp"
                android:text="Mes information Personnel"
                android:textColor="#000"
                android:textSize="16dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.074"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.163" />

            <Button
                android:id="@+id/btn_Mon_Profile"
                android:layout_width="match_parent"
                android:layout_height="69dp"
                android:layout_marginTop="2dp"
                android:background="@drawable/btn_blanc"
                android:gravity="fill_vertical"
                android:hapticFeedbackEnabled="false"
                android:shadowColor="#fff"
                android:text="    Mon Compte"
                android:textAllCaps="false"
                android:textIsSelectable="false"
                android:textSize="17dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.253" />


            <Button
                android:id="@+id/btn_favirie"
                android:layout_width="match_parent"
                android:layout_height="69dp"
                android:layout_marginTop="2dp"
                android:background="@drawable/btn_blanc"
                android:gravity="fill_vertical"
                android:hapticFeedbackEnabled="false"
                android:text="    Mes favorite"
                android:textAllCaps="false"
                android:textIsSelectable="false"
                android:textSize="17dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.253" />


            <Button
                android:id="@+id/btn_vendre_2"
                android:layout_width="match_parent"
                android:layout_height="69dp"
                android:layout_marginTop="2dp"
                android:background="@drawable/btn_blanc"
                android:textSize="17dp"
                android:textAllCaps="false"
                android:textIsSelectable="false"
                android:gravity="fill_vertical"
                android:hapticFeedbackEnabled="false"
                android:text="     Deposser une annonce"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.253" />


            <Button
                android:id="@+id/btn_condiscion"
                android:layout_width="match_parent"
                android:layout_height="69dp"
                android:layout_marginTop="2dp"
                android:background="@drawable/btn_blanc"
                android:textSize="17dp"
                android:textAllCaps="false"
                android:textIsSelectable="false"
                android:gravity="fill_vertical"
                android:hapticFeedbackEnabled="false"
                android:shadowColor="#fff"
                android:text="    Condition d'utilisation"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.566" />


            <Button
                android:id="@+id/btn_R_Discorde"
                android:layout_width="match_parent"
                android:layout_height="69dp"
                android:layout_marginTop="2dp"
                android:background="@drawable/btn_blanc"
                android:gravity="fill_vertical"
                android:hapticFeedbackEnabled="false"
                android:textSize="17dp"
                android:textIsSelectable="false"
                android:shadowColor="#FFFFFF"
                android:textAllCaps="false"
                android:text="    Rejoindre notre Discorde"
                android:textColor="#6D0CAA"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.673" />


            <Button
                android:id="@+id/btn_2_deconection"
                android:layout_width="343dp"
                android:layout_height="74dp"
                android:background="@drawable/btn_blanc"
                android:gravity="center"
                android:hapticFeedbackEnabled="false"
                android:layout_gravity="center"
                android:layout_marginTop="99dp"
                android:text="Deconnextion"
                android:textColor="#FF0000"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="1.0" />


        </LinearLayout>
    </ScrollView>





</androidx.constraintlayout.widget.ConstraintLayout>